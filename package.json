{
  "name": "nala",
  "version": "1.0.0",
  "description": "Automated E2E & integration testing of Milo-based projects.",
  "main": "index.js",
  "scripts": {
    "test": "npx playwright test",
    "test:gui": "npx playwright test --headed",
    "test:chrome": "playwright test --project=chromium --headed",
    "test:firefox": "playwright test --project=firefox --headed",
    "test:safari": "playwright test --project=webkit --headed",
    "test:chrome-headless": "playwright test --project=chromium",
    "test:firefox-headless": "playwright test --project=firefox",
    "test:safari-headless": "playwright test --project=webkit",
    "test.sauce.us": "saucectl run --region us-west-1",
    "test.sauce.eu": "saucectl run",
    "postinstall": "npm update browserstack-node-sdk",
    "test:browserstack": "npx browserstack-node-sdk playwright test",
    "test:bstack-acom": "npx browserstack-node-sdk playwright test --config='./configs/acom.config.js'",
    "test:bstack-bacom-blog": "npx browserstack-node-sdk playwright test --config='./configs/bacom-blog.config.js'",
    "test:bstack-bacom": "npx browserstack-node-sdk playwright test --config='./configs/bacom.config.js'",
    "test:bstack-blog": "npx browserstack-node-sdk playwright test --config='./configs/blog.config.js'",
    "test:bstack-cc": "npx browserstack-node-sdk playwright test --config='./configs/cc.config.js'",
    "test:bstack-dc": "npx browserstack-node-sdk playwright test --config='./configs/dc.config.js'",
    "test:bstack-feds": "npx browserstack-node-sdk playwright test --config='./configs/feds.config.js'",
    "test:bstack-helpx": "npx browserstack-node-sdk playwright test --config='./configs/helpx.config.js'",
    "test:bstack-milo": "npx browserstack-node-sdk playwright test --config='./configs/milo.config.js'",
    "test:bstack-uar": "npx browserstack-node-sdk playwright test --config='./configs/uar.config.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adobecom/nala.git"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobecom/nala/issues"
  },
  "homepage": "https://github.com/adobecom/nala#readme",
  "dependencies": {
    "@axe-core/playwright": "^4.7.0",
    "@playwright/test": "^1.38.1",
    "axe-html-reporter": "^2.2.3",
    "axios": "^1.6.5",
    "css": "^3.0.0",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.21.3",
    "@babel/eslint-parser": "7.21.3",
    "browserstack-node-sdk": "^1.31.23",
    "eslint": "8.37.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.27.5",
    "saucectl": "^0.169.0"
  }
}
